<svg width="300" height="600" viewBox="0 0 300 600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <style>
      .screen-text { font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif; }
    </style>
  </defs>
  
  <!-- Phone frame -->
  <rect width="300" height="600" rx="20" fill="#1a1a1a"/>
  <rect x="10" y="10" width="280" height="580" rx="15" fill="#f5f5f5"/>
  
  <!-- Status bar -->
  <rect x="10" y="10" width="280" height="30" rx="15" fill="#5f432c"/>
  <text x="150" y="30" class="screen-text" font-size="12" fill="#fff" text-anchor="middle">9:41 AM</text>
  
  <!-- App header -->
  <rect x="10" y="45" width="280" height="50" fill="#fff"/>
  <text x="150" y="75" class="screen-text" font-size="18" font-weight="700" fill="#5f432c" text-anchor="middle">Parsed Answers</text>
  
  <!-- Stats bar -->
  <rect x="20" y="110" width="260" height="40" rx="8" fill="#fff" stroke="#5f432c" stroke-width="2"/>
  <text x="70" y="135" class="screen-text" font-size="14" font-weight="600" fill="#21A179" text-anchor="middle">15 ✓</text>
  <text x="150" y="135" class="screen-text" font-size="14" font-weight="600" fill="#ef4444" text-anchor="middle">3 ✗</text>
  <text x="230" y="135" class="screen-text" font-size="14" font-weight="600" fill="#f59e0b" text-anchor="middle">2 —</text>
  
  <!-- Two-column answer grid -->
  <g transform="translate(20, 165)">
    <!-- Column 1 -->
    <!-- Correct answer (green) -->
    <rect x="0" y="0" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="10" y="23" class="screen-text" font-size="14" font-weight="600" fill="#333">1.</text>
    <text x="65" y="23" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">A</text>
    
    <rect x="0" y="40" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="10" y="63" class="screen-text" font-size="14" font-weight="600" fill="#333">2.</text>
    <text x="65" y="63" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">B</text>
    
    <!-- Incorrect answer (red) -->
    <rect x="0" y="80" width="125" height="35" rx="6" fill="#FFCDD2" stroke="#ef4444" stroke-width="2"/>
    <text x="10" y="103" class="screen-text" font-size="14" font-weight="600" fill="#333">3.</text>
    <text x="65" y="103" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">D</text>
    
    <rect x="0" y="120" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="10" y="143" class="screen-text" font-size="14" font-weight="600" fill="#333">4.</text>
    <text x="65" y="143" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">C</text>
    
    <rect x="0" y="160" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="10" y="183" class="screen-text" font-size="14" font-weight="600" fill="#333">5.</text>
    <text x="65" y="183" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">A</text>
    
    <!-- Blank answer (yellow) -->
    <rect x="0" y="200" width="125" height="35" rx="6" fill="#FFF9C4" stroke="#f59e0b" stroke-width="2"/>
    <text x="10" y="223" class="screen-text" font-size="14" font-weight="600" fill="#333">6.</text>
    <text x="65" y="223" class="screen-text" font-size="16" font-weight="700" fill="#999" text-anchor="middle">—</text>
    
    <rect x="0" y="240" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="10" y="263" class="screen-text" font-size="14" font-weight="600" fill="#333">7.</text>
    <text x="65" y="263" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">B</text>
    
    <rect x="0" y="280" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="10" y="303" class="screen-text" font-size="14" font-weight="600" fill="#333">8.</text>
    <text x="65" y="303" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">D</text>
    
    <!-- Column 2 -->
    <rect x="135" y="0" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="145" y="23" class="screen-text" font-size="14" font-weight="600" fill="#333">11.</text>
    <text x="200" y="23" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">A</text>
    
    <rect x="135" y="40" width="125" height="35" rx="6" fill="#FFCDD2" stroke="#ef4444" stroke-width="2"/>
    <text x="145" y="63" class="screen-text" font-size="14" font-weight="600" fill="#333">12.</text>
    <text x="200" y="63" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">A</text>
    
    <rect x="135" y="80" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="145" y="103" class="screen-text" font-size="14" font-weight="600" fill="#333">13.</text>
    <text x="200" y="103" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">C</text>
    
    <rect x="135" y="120" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="145" y="143" class="screen-text" font-size="14" font-weight="600" fill="#333">14.</text>
    <text x="200" y="143" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">B</text>
    
    <rect x="135" y="160" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="145" y="183" class="screen-text" font-size="14" font-weight="600" fill="#333">15.</text>
    <text x="200" y="183" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">D</text>
    
    <rect x="135" y="200" width="125" height="35" rx="6" fill="#C8E6C9" stroke="#21A179" stroke-width="2"/>
    <text x="145" y="223" class="screen-text" font-size="14" font-weight="600" fill="#333">16.</text>
    <text x="200" y="223" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">C</text>
    
    <rect x="135" y="240" width="125" height="35" rx="6" fill="#FFCDD2" stroke="#ef4444" stroke-width="2"/>
    <text x="145" y="263" class="screen-text" font-size="14" font-weight="600" fill="#333">17.</text>
    <text x="200" y="263" class="screen-text" font-size="16" font-weight="700" fill="#333" text-anchor="middle">B</text>
    
    <rect x="135" y="280" width="125" height="35" rx="6" fill="#FFF9C4" stroke="#f59e0b" stroke-width="2"/>
    <text x="145" y="303" class="screen-text" font-size="14" font-weight="600" fill="#333">18.</text>
    <text x="200" y="303" class="screen-text" font-size="16" font-weight="700" fill="#999" text-anchor="middle">—</text>
  </g>
  
  <!-- Action buttons -->
  <rect x="30" y="510" width="110" height="45" rx="8" fill="#fff" stroke="#5f432c" stroke-width="2"/>
  <text x="85" y="538" class="screen-text" font-size="14" font-weight="600" fill="#5f432c" text-anchor="middle">Edit</text>
  
  <rect x="160" y="510" width="110" height="45" rx="8" fill="#5f432c"/>
  <text x="215" y="538" class="screen-text" font-size="14" font-weight="600" fill="#fff" text-anchor="middle">Confirm</text>
</svg>
