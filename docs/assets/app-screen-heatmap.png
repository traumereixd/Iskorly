<svg width="300" height="600" viewBox="0 0 300 600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <style>
      .screen-text { font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif; }
    </style>
  </defs>
  
  <!-- Phone frame -->
  <rect width="300" height="600" rx="20" fill="#1a1a1a"/>
  <rect x="10" y="10" width="280" height="580" rx="15" fill="#f5f5f5"/>
  
  <!-- Status bar -->
  <rect x="10" y="10" width="280" height="30" rx="15" fill="#5f432c"/>
  <text x="150" y="30" class="screen-text" font-size="12" fill="#fff" text-anchor="middle">9:41 AM</text>
  
  <!-- App header -->
  <rect x="10" y="45" width="280" height="50" fill="#fff"/>
  <text x="150" y="75" class="screen-text" font-size="18" font-weight="700" fill="#5f432c" text-anchor="middle">Analytics</text>
  
  <!-- Overall stats -->
  <rect x="20" y="110" width="260" height="80" rx="8" fill="#fff" stroke="#5f432c" stroke-width="2"/>
  <text x="150" y="135" class="screen-text" font-size="14" fill="#666" text-anchor="middle">Class MPS</text>
  <text x="150" y="165" class="screen-text" font-size="36" font-weight="700" fill="#5f432c" text-anchor="middle">78%</text>
  <text x="150" y="185" class="screen-text" font-size="12" fill="#999" text-anchor="middle">20 students â€¢ 25 items</text>
  
  <!-- Item difficulty heatmap -->
  <g transform="translate(20, 205)">
    <text x="0" y="15" class="screen-text" font-size="14" font-weight="600" fill="#5f432c">Item Difficulty Heatmap</text>
    
    <!-- Legend -->
    <text x="0" y="35" class="screen-text" font-size="10" fill="#666">Easy</text>
    <rect x="35" y="28" width="15" height="10" rx="2" fill="#C8E6C9"/>
    
    <text x="60" y="35" class="screen-text" font-size="10" fill="#666">Medium</text>
    <rect x="110" y="28" width="15" height="10" rx="2" fill="#FFF9C4"/>
    
    <text x="135" y="35" class="screen-text" font-size="10" fill="#666">Hard</text>
    <rect x="170" y="28" width="15" height="10" rx="2" fill="#FFCDD2"/>
    
    <!-- Heatmap grid (5x5) -->
    <g transform="translate(0, 50)">
      <!-- Row 1 (Items 1-5) -->
      <rect x="0" y="0" width="50" height="40" rx="4" fill="#C8E6C9"/>
      <text x="25" y="15" class="screen-text" font-size="10" fill="#666" text-anchor="middle">Item 1</text>
      <text x="25" y="30" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">95%</text>
      
      <rect x="55" y="0" width="50" height="40" rx="4" fill="#C8E6C9"/>
      <text x="80" y="15" class="screen-text" font-size="10" fill="#666" text-anchor="middle">Item 2</text>
      <text x="80" y="30" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">90%</text>
      
      <rect x="110" y="0" width="50" height="40" rx="4" fill="#FFF9C4"/>
      <text x="135" y="15" class="screen-text" font-size="10" fill="#666" text-anchor="middle">Item 3</text>
      <text x="135" y="30" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">75%</text>
      
      <rect x="165" y="0" width="50" height="40" rx="4" fill="#C8E6C9"/>
      <text x="190" y="15" class="screen-text" font-size="10" fill="#666" text-anchor="middle">Item 4</text>
      <text x="190" y="30" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">85%</text>
      
      <rect x="220" y="0" width="40" height="40" rx="4" fill="#C8E6C9"/>
      <text x="240" y="15" class="screen-text" font-size="10" fill="#666" text-anchor="middle">5</text>
      <text x="240" y="30" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">92%</text>
      
      <!-- Row 2 (Items 6-10) -->
      <rect x="0" y="45" width="50" height="40" rx="4" fill="#FFF9C4"/>
      <text x="25" y="60" class="screen-text" font-size="10" fill="#666" text-anchor="middle">Item 6</text>
      <text x="25" y="75" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">70%</text>
      
      <rect x="55" y="45" width="50" height="40" rx="4" fill="#FFCDD2"/>
      <text x="80" y="60" class="screen-text" font-size="10" fill="#666" text-anchor="middle">Item 7</text>
      <text x="80" y="75" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">45%</text>
      
      <rect x="110" y="45" width="50" height="40" rx="4" fill="#C8E6C9"/>
      <text x="135" y="60" class="screen-text" font-size="10" fill="#666" text-anchor="middle">Item 8</text>
      <text x="135" y="75" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">88%</text>
      
      <rect x="165" y="45" width="50" height="40" rx="4" fill="#FFF9C4"/>
      <text x="190" y="60" class="screen-text" font-size="10" fill="#666" text-anchor="middle">Item 9</text>
      <text x="190" y="75" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">65%</text>
      
      <rect x="220" y="45" width="40" height="40" rx="4" fill="#C8E6C9"/>
      <text x="240" y="60" class="screen-text" font-size="10" fill="#666" text-anchor="middle">10</text>
      <text x="240" y="75" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">80%</text>
      
      <!-- Row 3 (Items 11-15) -->
      <rect x="0" y="90" width="50" height="40" rx="4" fill="#C8E6C9"/>
      <text x="25" y="105" class="screen-text" font-size="10" fill="#666" text-anchor="middle">11</text>
      <text x="25" y="120" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">82%</text>
      
      <rect x="55" y="90" width="50" height="40" rx="4" fill="#C8E6C9"/>
      <text x="80" y="105" class="screen-text" font-size="10" fill="#666" text-anchor="middle">12</text>
      <text x="80" y="120" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">78%</text>
      
      <rect x="110" y="90" width="50" height="40" rx="4" fill="#FFCDD2"/>
      <text x="135" y="105" class="screen-text" font-size="10" fill="#666" text-anchor="middle">13</text>
      <text x="135" y="120" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">55%</text>
      
      <rect x="165" y="90" width="50" height="40" rx="4" fill="#FFF9C4"/>
      <text x="190" y="105" class="screen-text" font-size="10" fill="#666" text-anchor="middle">14</text>
      <text x="190" y="120" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">68%</text>
      
      <rect x="220" y="90" width="40" height="40" rx="4" fill="#C8E6C9"/>
      <text x="240" y="105" class="screen-text" font-size="10" fill="#666" text-anchor="middle">15</text>
      <text x="240" y="120" class="screen-text" font-size="14" font-weight="700" fill="#333" text-anchor="middle">85%</text>
    </g>
  </g>
  
  <!-- Common wrong answers section -->
  <g transform="translate(20, 445)">
    <text x="0" y="15" class="screen-text" font-size="14" font-weight="600" fill="#5f432c">Common Mistakes</text>
    
    <rect x="0" y="25" width="260" height="30" rx="6" fill="#fff" stroke="#FFCDD2" stroke-width="2"/>
    <text x="10" y="47" class="screen-text" font-size="12" fill="#333">Item 7: 55% chose B (correct: C)</text>
    
    <rect x="0" y="60" width="260" height="30" rx="6" fill="#fff" stroke="#FFCDD2" stroke-width="2"/>
    <text x="10" y="82" class="screen-text" font-size="12" fill="#333">Item 13: 45% chose A (correct: D)</text>
  </g>
  
  <!-- Export button -->
  <rect x="30" y="530" width="240" height="45" rx="8" fill="#5f432c"/>
  <text x="150" y="558" class="screen-text" font-size="14" font-weight="600" fill="#fff" text-anchor="middle">Export CSV</text>
</svg>
